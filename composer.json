{
    "name": "lukashron/zbozicz-request-conversion",
    "description": "Request maker pro Zbozi.cz konverze.",
    "version": "1.0.1",
    "authors": [
        {
            "name": "Lukas Hron",
            "email": "info@lukashron.cz",
            "homepage": "https://www.lukashron.cz",
            "role": "Developer"
        }
    ],
    "type": "library",
    "license": "MIT",
    "keywords": [
        "zbozi",
