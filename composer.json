    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "rector/rector": "^0.12.23",
        "symfony/http-client": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "LukasHron\\ZboziCzRequestConversion\\": "src/"
        }
